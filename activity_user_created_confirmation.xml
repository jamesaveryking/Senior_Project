<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context="com.example.jamesking.choice.User_Created_Confirmation">


    <Button
        android:id="@+id/backButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:text="@string/back" />

    <Button
        android:id="@+id/loginButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/backButton"
        android:text="@string/login" />

    <TextView
        android:id="@+id/nameTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/userConfirmationTitleTextView"
        android:text="@string/name"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/loadedNameTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/nameTextView"
        android:layout_alignBottom="@+id/nameTextView"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/nameTextView"
        android:text="@string/placeholder"
        android:textAlignment="textEnd"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/usernameTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/nameTextView"
        android:text="@string/username"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/loadedUsernameTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/usernameTextView"
        android:layout_toEndOf="@+id/usernameTextView"
        android:text="@string/placeholder"
        android:textAlignment="textEnd"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/passwordTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/usernameTextView"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/usernameTextView"
        android:text="@string/password"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/loadedPasswordTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/passwordTextView"
        android:layout_alignBottom="@+id/passwordTextView"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/usernameTextView"
        android:text="@string/placeholder"
        android:textAlignment="textEnd"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/stateTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/passwordTextView"
        android:text="@string/state"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/loadedStateTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/stateTextView"
        android:layout_alignBottom="@+id/stateTextView"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/stateTextView"
        android:text="@string/placeholder"
        android:textAlignment="textEnd"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/cityTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/loadedStateTextView"
        android:text="@string/city"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/loadedCityTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/cityTextView"
        android:layout_toEndOf="@+id/cityTextView"
        android:text="@string/placeholder"
        android:textAlignment="textEnd"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/userConfirmationTitleTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:text="@string/userConfirmation_Title"
        android:textAlignment="center"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/sQ1TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sq1"
        android:textSize="20sp"
        android:layout_below="@+id/cityTextView"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/loadedSQ1TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/sQ1TextView"
        android:text="@string/placeholder"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/sQ1ATextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/loadedSQ1TextView"
        android:text="@string/answer"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/loadedSQ1ATextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/placeholder"
        android:textSize="20sp"
        android:layout_below="@+id/sQ1ATextView"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/sQ2TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/loadedSQ1ATextView"
        android:text="@string/sq2"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/loadedSQ2TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sQ2TextView"
        android:text="@string/placeholder"
        android:textSize="20sp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/sQ2ATextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/loadedSQ2TextView"
        android:text="@string/answer"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/loadedSQ2ATextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/sQ2ATextView"
        android:text="@string/placeholder"
        android:textSize="20sp"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/requestTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/request"
        android:textAlignment="center"
        android:textSize="20sp"
        android:layout_above="@+id/backButton"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />


</RelativeLayout>
